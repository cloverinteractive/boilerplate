{
  "gentypeconfig": {
    "language": "typescript",
    "module": "commonjs",
    "importPath": "relative",
    "shims": {
      "Js": "Js",
      "React": "ReactShim",
      "ReactEvent": "ReactEvent",
      "ReasonPervasives": "ReasonPervasives",
      "ReasonReact": "ReactShim"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  },
  "name": "boilerplate",
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "components",
          "subdirs": [
            {
              "dir": "__tests__",
              "type": "dev"
            }
          ]
        },
        {
          "dir": "data",
          "subdirs": true
        },
        {
          "dir": "pages",
          "subdirs": [
            {
              "dir": "__tests__",
              "type": "dev"
            }
          ]
        }
      ]
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-guid",
    "bs-enzyme",
    "re-classnames",
    "reason-react",
    "@moox/bs-react-helmet",
    "bs-css",
    "bs-css-emotion"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest",
    "bs-react-testing-library",
    "bs-react-test-renderer",
    "reason-hooks-testing-library"
  ],
  "warnings": {
    "error": "+101"
  },
  "namespace": true,
  "refmt": 3
}