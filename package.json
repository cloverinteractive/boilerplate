  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "lint": "eslint --ext .js --ext .jsx ./src",
    "lint:fix": "npm run lint --  --fix",
    "test:karma": "karma start --single-run",
    "test:karma:watch": "karma start",
    "test": "cross-env NODE_ENV=test npm run test:single -- --recursive test",
    "test:coverage": "nyc --exclude=test/ npm run test",
    "coverage": "nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "test:watch": "npm run test -- --watch",
    "test:single": "cross-env NODE_PATH=./src mocha --opts .mocha.opts",
    "start": "cross-env NODE_ENV=development BABEL_ENV=node NODE_PATH=./src nodemon ./src/server --exec babel-node",
    "start:prod": "cross-env NODE_ENV=production node build/bundle.js"
  }
