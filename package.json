  "scripts": {
    "build": "npm-run-all build:*",
    "build:rclient": "bsb -make-world",
    "build:tclient": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "check-types": "npx tsc",
    "lint": "npx eslint --ext .js --ext .jsx --ext .tsx ./src",
    "test:karma": "karma start --single-run",
    "test:karma:watch": "karma start",
    "test": "cross-env NODE_ENV=test npx mocha-webpack --opts .mocha.opts --webpack-config config/webpack/mocha.js --recursive",
    "test:coverage": "cross-env MOCHA_ENV=coverage nyc npm test",
    "coverage": "nyc report --reporter=text-lcov | npx coveralls",
    "start": "npm-run-all --parallel dev:*",
    "dev:reason": "bsb -make-world -w",
    "dev:tsx": "cross-env NODE_ENV=development BABEL_ENV=node NODE_PATH=./src nodemon ./src/server --exec babel-node --extensions \".ts,.tsx,.js,.jsx\"",
    "start:prod": "cross-env NODE_ENV=production node build/bundle.js"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "private": true
}
